mdifne nvim ~/.config
mdifne nvim ~/.config/nvim
lnifne nvim ~/.config/nvim/init.lua ~/.dotfiles/config/nvim/init.lua
lnifne nvim ~/.config/nvim/lua ~/.dotfiles/config/nvim/lua
lnifne nvim ~/.config/nvim/spell ~/.dotfiles/config/nvim/spell
lnifne nvim ~/.config/nvim/snippets ~/.dotfiles/config/nvim/snippets
lnifne nvim ~/.config/nvim/ftplugin ~/.dotfiles/config/nvim/ftplugin

